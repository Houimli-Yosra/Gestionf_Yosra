<Window x:Class="Gestionf_Yosra.Controls.PersonneControls"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gestionf_Yosra.Controls"
        mc:Ignorable="d"
        Title="PersonneControls" Height="450" Width="800">
    <Grid>
        <Canvas>
            <Label Content="Nom" Canvas.Left="94" Canvas.Top="68"/>
            <TextBox x:Name="textBox3" Width="250" Text="{Binding Path=Nom}" Height="20" Canvas.Left="209" Canvas.Top="68"/>
            <Label Content="Prenom" Canvas.Left="82" Canvas.Top="113"/>
            <TextBox x:Name="textBox2" Width="250" Text="{Binding Path=Prenom}" Height="20" Canvas.Left="209" Canvas.Top="113"/>
            <Label Content="Age" Canvas.Left="70" Canvas.Top="146"/>
            <TextBox x:Name="textBox1" Width="250" Text="{Binding Path=Age}" Height="20" Canvas.Left="209" Canvas.Top="152"/>
            <Button Content="Enregistrer" Width="75" Canvas.Left="515" Canvas.Top="249" Click="Button_Click"/>

          

        </Canvas>
    </Grid>
</Window>
